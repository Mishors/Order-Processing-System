	<queryString language="SQL">
		<![CDATA[select * 
from users join (select t.cstmr_email,count(*) as c from (select *  from customer_orders where timestampdiff(DAY,sale_date,now()) <= 90) as t
group by t.cstmr_email 
LIMIT 5) as temp on users.email = temp.cstmr_email
order by temp.c DESC]]>
